    label.Text = "üçØ Using Honey Machine..."
    game.ReplicatedStorage.GameEvents.HoneyMachineService_RE:FireServer("MachineInteract")
    label.Text = "‚úÖ Honey Used!"
end)


createButton("üõ∞ V1349: (Join Server1)", Color3.fromRGB(180, 120, 255), function()
    local serverId = "843cacc3-cf2f-4e12-875c-814b511d0d3e"
    label.Text = "üõ∞ Warping to V1349..."
    local success, err = pcall(function()
        TeleportService:TeleportToPlaceInstance(placeId, serverId, player)
    end)
    if not success then
        label.Text = string.find(err, "full") and "üö´ Server Full (in queue)" or "‚ùå Error: " .. tostring(err)
    end
end)

