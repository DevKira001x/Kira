logoButton.MouseButton1Click:Connect(function()
	mainGui.Enabled = not mainGui.Enabled
end)

local dragging, dragStart, startPos, dragInput
logoButton.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
		dragging = true
		dragStart = input.Position
		startPos = logoButton.Position
		dragInput = input
		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then dragging = false end
		end)
	end
end)

UserInputService.InputChanged:Connect(function(input)
	if input == dragInput and dragging then
		local delta = input.Position - dragStart
		local newX = math.clamp(startPos.X.Offset + delta.X, 0, workspace.CurrentCamera.ViewportSize.X - logoButton.AbsoluteSize.X)
		local newY = math.clamp(startPos.Y.Offset + delta.Y, 0, workspace.CurrentCamera.ViewportSize.Y - logoButton.AbsoluteSize.Y)
		logoButton.Position = UDim2.new(0, newX, 0, newY)
	end
end)

local frameDragging, frameDragStart, frameStartPos, frameDragInput
frame.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
		frameDragging = true
		frameDragStart = input.Position
		frameStartPos = frame.Position
		frameDragInput = input
		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then frameDragging = false end
		end)
	end
end)

UserInputService.InputChanged:Connect(function(input)
	if input == frameDragInput and frameDragging then
		local delta = input.Position - frameDragStart
		local newX = math.clamp(frameStartPos.X.Offset + delta.X, 0, workspace.CurrentCamera.ViewportSize.X - frame.AbsoluteSize.X)
		local newY = math.clamp(frameStartPos.Y.Offset + delta.Y, 0, workspace.CurrentCamera.ViewportSize.Y - frame.AbsoluteSize.Y)
		frame.Position = UDim2.new(0, newX, 0, newY)
	end
end)

local function createButton(text, color, callback)
	local button = Instance.new("TextButton")
	button.Size = UDim2.new(1, 0, 0, 32)
	button.Text = text
	button.Font = Enum.Font.GothamBold
	button.TextSize = 13
	button.TextColor3 = Color3.new(1, 1, 1)
	button.BackgroundColor3 = color
	button.AutoButtonColor = true
	button.ZIndex = 1
	button.Parent = scroll
	Instance.new("UICorner", button).CornerRadius = UDim.new(0, 6)
	Instance.new("UIStroke", button).Color = Color3.fromRGB(0, 0, 0)
	button.MouseButton1Click:Connect(function()
		callback(button)
	end)
	return button
end

local function clearButtons()
	for _, child in ipairs(scroll:GetChildren()) do
		if child:IsA("TextButton") or child:IsA("TextBox") then
			child:Destroy()
		end
	end
end

local function loadNoKeyTab()
	clearButtons()
	createButton("âš¡ Speed Hub", Color3.fromRGB(255, 120, 0), function()
		label.Text = "âš¡ Loading Speed Hub..."
		loadstring(game:HttpGet("https://raw.githubusercontent.com/AhmadV99/Speed-Hub-X/main/Speed%20Hub%20X.lua", true))()
	end)
	createButton("ðŸ”® Nat hub", Color3.fromRGB(120, 180, 255), function()
		label.Text = "ðŸ”® Loading Nathub..."
		loadstring(game:HttpGet("https://get.nathub.xyz/loader"))()
	end)
	createButton("ðŸ›  NoLag Hub", Color3.fromRGB(255, 100, 100), function()
		label.Text = "ðŸ›  Loading NoLag Hub..."
		loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/NoLag-id/No-Lag-HUB/refs/heads/main/Loader/LoaderV1.lua"))()
	end)
end

local function loadKeyTab()
	clearButtons()
	createButton("ðŸŒ™ Lunor Hub", Color3.fromRGB(180, 100, 255), function()
		label.Text = "ðŸŒ™ Loading Lunor Hub..."
		loadstring(game:HttpGet("https://lunor.dev/loader"))()
	end)
end

noKeyTab.MouseButton1Click:Connect(function()
	noKeyTab.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
	keyTab.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
	loadNoKeyTab()
end)

keyTab.MouseButton1Click:Connect(function()
	keyTab.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
	noKeyTab.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
	loadKeyTab()
end)

loadNoKeyTab()


loadstring(game:HttpGet("https://raw.githubusercontent.com/DevKira001x/Kira/refs/heads/main/Ke"))();
