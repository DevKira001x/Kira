-- Kira001x
local TeleportService = game:GetService("TeleportService")
local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")

local player = Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

local logoGui = Instance.new("ScreenGui", playerGui)
logoGui.Name = "KiraHubLogoToggle"
logoGui.ResetOnSpawn = false
logoGui.IgnoreGuiInset = true

local logoButton = Instance.new("ImageButton", logoGui)
logoButton.Size = UDim2.new(0, 48, 0, 48)
logoButton.Position = UDim2.new(0, 16, 1, -64)
logoButton.BackgroundTransparency = 1
logoButton.Image = "rbxassetid://18402198349"
logoButton.ZIndex = 10

local mainGui = Instance.new("ScreenGui", playerGui)
mainGui.Name = "KiraHubUI"
mainGui.ResetOnSpawn = false
mainGui.IgnoreGuiInset = true
mainGui.ZIndexBehavior = Enum.ZIndexBehavior.Global
mainGui.Enabled = false

local frame = Instance.new("Frame", mainGui)
frame.Size = UDim2.new(0, 240, 0, 240)
frame.Position = UDim2.new(0, 20, 0, 80)
frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
frame.Active = true
frame.Draggable = true
Instance.new("UICorner", frame).CornerRadius = UDim.new(0, 10)
Instance.new("UIStroke", frame).Color = Color3.fromRGB(255, 255, 255)

local label = Instance.new("TextLabel", frame)
label.Size = UDim2.new(1, -20, 0, 22)
label.Position = UDim2.new(0, 10, 0, 6)
label.Text = "Kira Hub"
label.Font = Enum.Font.GothamBold
label.TextSize = 13
label.TextColor3 = Color3.new(1, 1, 1)
label.BackgroundTransparency = 1
label.TextXAlignment = Enum.TextXAlignment.Left

local tabFrame = Instance.new("Frame", frame)
tabFrame.Size = UDim2.new(1, -20, 0, 26)
tabFrame.Position = UDim2.new(0, 10, 0, 30)
tabFrame.BackgroundTransparency = 1
tabFrame.ZIndex = 2

local function createTabButton(name, pos)
	local button = Instance.new("TextButton", tabFrame)
	button.Size = UDim2.new(1/3, -4, 1, 0)
	button.Position = UDim2.new(pos, pos * 2, 0, 0)
	button.Text = name
	button.Font = Enum.Font.GothamBold
	button.TextSize = 13
	button.TextColor3 = Color3.new(1, 1, 1)
	button.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
	Instance.new("UICorner", button).CornerRadius = UDim.new(0, 6)
	return button
end

local menuTab = createTabButton("Menu", 0)
menuTab.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
local shopTab = createTabButton("Shop", 1/3)
local stripTab = createTabButton("Strip", 2/3)

local scroll = Instance.new("ScrollingFrame", frame)
scroll.Position = UDim2.new(0, 10, 0, 60)
scroll.Size = UDim2.new(1, -20, 1, -106)
scroll.CanvasSize = UDim2.new(0, 0, 0, 0)
scroll.ScrollBarThickness = 4
scroll.BackgroundTransparency = 1
scroll.AutomaticCanvasSize = Enum.AutomaticSize.Y
scroll.ClipsDescendants = true

local layout = Instance.new("UIListLayout", scroll)
layout.Padding = UDim.new(0, 6)
layout.SortOrder = Enum.SortOrder.LayoutOrder

local credit = Instance.new("TextLabel", frame)
credit.Size = UDim2.new(1, -10, 0, 20)
credit.Position = UDim2.new(0, 5, 1, -24)
credit.Text = "¬© ‡πÄ‡∏Ç‡∏µ‡∏¢‡∏ô‡πÇ‡∏î‡∏¢ Kira001x"
credit.Font = Enum.Font.Gotham
credit.TextSize = 11
credit.TextColor3 = Color3.new(1, 1, 1)
credit.BackgroundTransparency = 1
credit.TextXAlignment = Enum.TextXAlignment.Right

logoButton.MouseButton1Click:Connect(function()
	mainGui.Enabled = not mainGui.Enabled
end)

-- Make logo draggable
local dragging, dragInput, dragStart, startPos
logoButton.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 then
		dragging = true
		dragStart = input.Position
		startPos = logoButton.Position
		dragInput = input
		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then dragging = false end
		end)
	end
end)

UserInputService.InputChanged:Connect(function(input)
	if input == dragInput and dragging then
		local delta = input.Position - dragStart
		logoButton.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end
end)

local function clearButtons()
	for _, child in ipairs(scroll:GetChildren()) do
		if child:IsA("TextButton") or child:IsA("TextBox") or child:IsA("Frame") then
			child:Destroy()
		end
	end
end

local function createButton(text, color, callback)
	local button = Instance.new("TextButton")
	button.Size = UDim2.new(1, 0, 0, 32)
	button.Text = text
	button.Font = Enum.Font.GothamBold
	button.TextSize = 13
	button.TextColor3 = Color3.new(1, 1, 1)
	button.BackgroundColor3 = color
	button.AutoButtonColor = true
	button.ZIndex = 1
	button.Parent = scroll
	Instance.new("UICorner", button).CornerRadius = UDim.new(0, 6)
	Instance.new("UIStroke", button).Color = Color3.fromRGB(0, 0, 0)
	button.MouseButton1Click:Connect(function() callback(button) end)
	return button
end

-- Menu tab logic
local pickup_enabled = false
local auto_all_enabled = false

local function loadMenuTab()
	clearButtons()

	createButton("Auto Collect: OFF", Color3.fromRGB(100, 200, 100), function(button)
		pickup_enabled = not pickup_enabled
		button.Text = "Auto Collect: " .. (pickup_enabled and "ON" or "OFF")

		if pickup_enabled then
			task.spawn(function()
				local CollectController = require(game:GetService("ReplicatedStorage").Modules.CollectController)
				local workspace_ref = game:GetService("Workspace")
				local local_player = game:GetService("Players").LocalPlayer
				CollectController._lastCollected = 0
				CollectController._holding = true
				CollectController:_updateButtonState()
				local farm_model
				for _, d in next, workspace_ref:FindFirstChild("Farm"):GetDescendants() do
					if d.Name == "Owner" and d.Value == local_player.Name then
						farm_model = d.Parent and d.Parent.Parent
						break
					end
				end
				while pickup_enabled and farm_model do
					local plants = farm_model:FindFirstChild("Plants_Physical")
					if plants then
						for _, p in next, plants:GetChildren() do
							if p:IsA("Model") then
								CollectController:Collect(p)
								for _, o in next, p:GetDescendants() do
									CollectController:Collect(o)
									task.wait(0.01)
								end
							end
						end
					end
					task.wait(0.01)
				end
			end)
		end
	end)

	createButton("Auto Submit All Every 10s: OFF", Color3.fromRGB(150, 150, 255), function(button)
		auto_all_enabled = not auto_all_enabled
		button.Text = "Auto Submit All Every 10s: " .. (auto_all_enabled and "ON" or "OFF")

		if auto_all_enabled then
			task.spawn(function()
				while auto_all_enabled do
					local args = {"SubmitAllPlants"}
					game:GetService("ReplicatedStorage"):WaitForChild("GameEvents"):WaitForChild("SummerHarvestRemoteEvent"):FireServer(unpack(args))
					task.wait(10)
				end
			end)
		end
	end)
end

local function loadShopTab()
	clearButtons()
end

local function loadScriptTab()
	clearButtons()
	createButton("‚ö° Speed Hub", Color3.fromRGB(255, 120, 0), function()
		label.Text = "‚ö° Loading Speed Hub..."
		loadstring(game:HttpGet("https://raw.githubusercontent.com/AhmadV99/Speed-Hub-X/main/Speed%20Hub%20X.lua", true))()
	end)
	createButton("üîÆ Nat hub", Color3.fromRGB(120, 180, 255), function()
		label.Text = "üîÆ Loading Nathub..."
		loadstring(game:HttpGet("https://get.nathub.xyz/loader"))()
	end)
	createButton("üõ† NoLag Hub", Color3.fromRGB(255, 100, 100), function()
		label.Text = "üõ† Loading NoLag Hub..."
		loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/NoLag-id/No-Lag-HUB/refs/heads/main/Loader/LoaderV1.lua"))()
	end)
	createButton("üåô Lunor Hub", Color3.fromRGB(180, 100, 255), function()
		label.Text = "üåô Loading Lunor Hub..."
		loadstring(game:HttpGet("https://lunor.dev/loader"))()
	end)
end

menuTab.MouseButton1Click:Connect(function()
	menuTab.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
	shopTab.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
	stripTab.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
	loadMenuTab()
end)

shopTab.MouseButton1Click:Connect(function()
	shopTab.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
	menuTab.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
	stripTab.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
	loadShopTab()
end)

stripTab.MouseButton1Click:Connect(function()
	stripTab.BackgroundColor3 = Color3.fromRGB(80, 80, 80)
	menuTab.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
	shopTab.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
	loadStripTab()
end)

loadMenuTab()
