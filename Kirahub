--Kira001x
local TeleportService = game:GetService("TeleportService")
local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")

local player = Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")
local placeId = game.PlaceId

local startTime = tick()
local isKicked = false
local kickEnabled = true
local correctPassword = "1142"

local loopingSeed = false
local loopingGear = false

local logoGui = Instance.new("ScreenGui", playerGui)
logoGui.Name = "KiraHubLogoToggle"
logoGui.ResetOnSpawn = false
logoGui.IgnoreGuiInset = true

local logoButton = Instance.new("ImageButton", logoGui)
logoButton.Size = UDim2.new(0, 48, 0, 48)
logoButton.Position = UDim2.new(0, 16, 1, -64)
logoButton.BackgroundTransparency = 1
logoButton.Image = "rbxassetid://18402198349"
logoButton.ZIndex = 10

local mainGui = Instance.new("ScreenGui", playerGui)
mainGui.Name = "KiraHubUI"
mainGui.ResetOnSpawn = false
mainGui.IgnoreGuiInset = true
mainGui.ZIndexBehavior = Enum.ZIndexBehavior.Global
mainGui.Enabled = false

local frame = Instance.new("Frame", mainGui)
frame.Size = UDim2.new(0, 240, 0, 320)
frame.Position = UDim2.new(0, 20, 0, 80)
frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
frame.Active = true
Instance.new("UICorner", frame).CornerRadius = UDim.new(0, 10)
Instance.new("UIStroke", frame).Color = Color3.fromRGB(255, 255, 255)

local label = Instance.new("TextLabel", frame)
label.Size = UDim2.new(1, -20, 0, 22)
label.Position = UDim2.new(0, 10, 0, 6)
label.Text = "Kira Hub - 60s"
label.Font = Enum.Font.GothamBold
label.TextSize = 13
label.TextColor3 = Color3.new(1, 1, 1)
label.BackgroundTransparency = 1
label.TextXAlignment = Enum.TextXAlignment.Left

local scroll = Instance.new("ScrollingFrame", frame)
scroll.Position = UDim2.new(0, 10, 0, 34)
scroll.Size = UDim2.new(1, -20, 1, -80)
scroll.CanvasSize = UDim2.new(0, 0, 0, 0)
scroll.ScrollBarThickness = 4
scroll.BackgroundTransparency = 1
scroll.AutomaticCanvasSize = Enum.AutomaticSize.Y
scroll.ClipsDescendants = true

local layout = Instance.new("UIListLayout", scroll)
layout.Padding = UDim.new(0, 6)
layout.SortOrder = Enum.SortOrder.LayoutOrder

local credit = Instance.new("TextLabel", frame)
credit.Size = UDim2.new(1, -10, 0, 20)
credit.Position = UDim2.new(0, 5, 1, -24)
credit.Text = "¬© ‡πÄ‡∏Ç‡∏µ‡∏¢‡∏ô‡πÇ‡∏î‡∏¢ Kira001x"
credit.Font = Enum.Font.Gotham
credit.TextSize = 11
credit.TextColor3 = Color3.new(1, 1, 1)
credit.BackgroundTransparency = 1
credit.TextXAlignment = Enum.TextXAlignment.Right

logoButton.MouseButton1Click:Connect(function()
	mainGui.Enabled = not mainGui.Enabled
end)

local dragging, dragStart, startPos, dragInput
logoButton.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
		dragging = true
		dragStart = input.Position
		startPos = logoButton.Position
		dragInput = input
		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then
				dragging = false
			end
		end)
	end
end)

UserInputService.InputChanged:Connect(function(input)
	if input == dragInput and dragging then
		local delta = input.Position - dragStart
		logoButton.Position = UDim2.new(0, math.clamp(startPos.X.Offset + delta.X, 0, workspace.CurrentCamera.ViewportSize.X - logoButton.AbsoluteSize.X), 0, math.clamp(startPos.Y.Offset + delta.Y, 0, workspace.CurrentCamera.ViewportSize.Y - logoButton.AbsoluteSize.Y))
	end
end)

local frameDragging, frameDragStart, frameStartPos, frameDragInput
frame.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
		frameDragging = true
		frameDragStart = input.Position
		frameStartPos = frame.Position
		frameDragInput = input
		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then
				frameDragging = false
			end
		end)
	end
end)

UserInputService.InputChanged:Connect(function(input)
	if input == frameDragInput and frameDragging then
		local delta = input.Position - frameDragStart
		frame.Position = UDim2.new(0, math.clamp(frameStartPos.X.Offset + delta.X, 0, workspace.CurrentCamera.ViewportSize.X - frame.AbsoluteSize.X), 0, math.clamp(frameStartPos.Y.Offset + delta.Y, 0, workspace.CurrentCamera.ViewportSize.Y - frame.AbsoluteSize.Y))
	end
end)

local function createButton(text, color, callback)
	local button = Instance.new("TextButton")
	button.Size = UDim2.new(1, 0, 0, 32)
	button.Text = text
	button.Font = Enum.Font.GothamBold
	button.TextSize = 13
	button.TextColor3 = Color3.new(1, 1, 1)
	button.BackgroundColor3 = color
	button.AutoButtonColor = true
	button.ZIndex = 1
	button.Parent = scroll
	Instance.new("UICorner", button).CornerRadius = UDim.new(0, 6)
	Instance.new("UIStroke", button).Color = Color3.fromRGB(0, 0, 0)
	button.MouseButton1Click:Connect(function()
		callback(button)
	end)
	return button
end

createButton("üöÄ Auto Skip Seed: OFF", Color3.fromRGB(0, 170, 255), function(button)
	loopingSeed = not loopingSeed
	button.Text = loopingSeed and "üöÄ Auto Skip Seed: ON" or "üöÄ Auto Skip Seed: OFF"
	label.Text = button.Text
end)

createButton("‚öôÔ∏è Auto Skip Gear: OFF", Color3.fromRGB(0, 170, 255), function(button)
	loopingGear = not loopingGear
	button.Text = loopingGear and "‚öôÔ∏è Auto Skip Gear: ON" or "‚öôÔ∏è Auto Skip Gear: OFF"
	label.Text = button.Text
end)

createButton("üíª AutoCraft NoKey ¬©NoLag", Color3.fromRGB(150, 200, 255), function()
	label.Text = "üì¶ Loading NoLag Script..."
	loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/NoLag-id/No-Lag-HUB/refs/heads/main/Loader/LoaderV1.lua"))()
end)

createButton("üçØ Click Honey", Color3.fromRGB(255, 200, 0), function()
	game.ReplicatedStorage.GameEvents.HoneyMachineService_RE:FireServer("MachineInteract")
	label.Text = "‚úÖ Honey Used!"
end)

createButton("üõ∞ V1349: (Join Server1)", Color3.fromRGB(180, 120, 255), function()
	local sid = "843cacc3-cf2f-4e12-875c-814b511d0d3e"
	local success, err = pcall(function()
		TeleportService:TeleportToPlaceInstance(placeId, sid, player)
	end)
	label.Text = success and "‚úÖ Warped!" or "‚ùå Error: " .. tostring(err)
end)

createButton("üìã Copy Server ID", Color3.fromRGB(100, 200, 100), function()
	local id = game.JobId
	pcall(setclipboard, id)
	label.Text = "üìã Copied: " .. id
end)

local serverInput = Instance.new("TextBox", scroll)
serverInput.Size = UDim2.new(1, 0, 0, 32)
serverInput.PlaceholderText = "üîë Enter Server ID to Join"
serverInput.Text = ""
serverInput.Font = Enum.Font.Gotham
serverInput.TextSize = 13
serverInput.TextColor3 = Color3.new(1, 1, 1)
serverInput.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
serverInput.ClearTextOnFocus = false
serverInput.TextXAlignment = Enum.TextXAlignment.Left
Instance.new("UICorner", serverInput).CornerRadius = UDim.new(0, 6)
Instance.new("UIStroke", serverInput).Transparency = 0.4

createButton("üîÑ Join Server by ID", Color3.fromRGB(255, 170, 0), function()
	local sid = serverInput.Text
	if sid ~= "" then
		local success, err = pcall(function()
			TeleportService:TeleportToPlaceInstance(placeId, sid, player)
		end)
		label.Text = success and "‚úÖ Warped!" or "‚ùå Failed: " .. tostring(err)
	else
		label.Text = "‚ö†Ô∏è ‡∏Å‡∏£‡∏∏‡∏ì‡∏≤‡πÉ‡∏™‡πà Server ID ‡∏Å‡πà‡∏≠‡∏ô!"
	end
end)

local passwordInput = Instance.new("TextBox", scroll)
passwordInput.Size = UDim2.new(1, 0, 0, 32)
passwordInput.PlaceholderText = "üîí ‡πÉ‡∏™‡πà‡∏£‡∏´‡∏±‡∏™‡∏ú‡πà‡∏≤‡∏ô‡∏Å‡πà‡∏≠‡∏ô‡∏õ‡∏¥‡∏î Timer"
passwordInput.Text = ""
passwordInput.Font = Enum.Font.Gotham
passwordInput.TextSize = 13
passwordInput.TextColor3 = Color3.new(1, 1, 1)
passwordInput.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
passwordInput.ClearTextOnFocus = false
passwordInput.TextXAlignment = Enum.TextXAlignment.Left
Instance.new("UICorner", passwordInput).CornerRadius = UDim.new(0, 6)
Instance.new("UIStroke", passwordInput).Transparency = 0.4

local kickButton = createButton("‚è≥ Kick Timer: ON", Color3.fromRGB(255, 70, 70), function()
	if kickEnabled then
		if passwordInput.Text == correctPassword then
			kickEnabled = false
			kickButton.Text = "‚è∏ Kick Timer: OFF"
			label.Text = kickButton.Text
		else
			label.Text = "‚ùå ‡∏ï‡πâ‡∏≠‡∏á‡πÉ‡∏™‡πà‡∏£‡∏´‡∏±‡∏™‡∏ú‡πà‡∏≤‡∏ô‡∏Å‡πà‡∏≠‡∏ô‡∏õ‡∏¥‡∏î!"
		end
	else
		kickEnabled = true
		startTime = tick()
		isKicked = false
		kickButton.Text = "‚è≥ Kick Timer: ON"
		label.Text = kickButton.Text
	end
end)

task.spawn(function()
	while true do
		task.wait(1)
		if kickEnabled and not isKicked then
			local remaining = 1000 - math.floor(tick() - startTime)
			if remaining <= 0 then
				isKicked = true
				game.StarterGui:SetCore("ChatMakeSystemMessage", {
					Text = "‚è∞ ‡∏Ñ‡∏∏‡∏ì‡∏≠‡∏¢‡∏π‡πà‡∏Ñ‡∏£‡∏ö‡πÄ‡∏ß‡∏•‡∏≤‡πÅ‡∏•‡πâ‡∏ß ‡πÉ‡∏´‡πâ‡∏Ñ‡∏ô‡∏≠‡∏∑‡πà‡∏ô‡πÄ‡∏Ç‡∏≤‡∏ö‡πâ‡∏≤‡∏á‡∏ô‡∏∞‡∏Ñ‡∏£‡∏±‡∏ö",
					Color = Color3.new(1, 1, 1),
					Font = Enum.Font.Gotham,
					FontSize = Enum.FontSize.Size24
				})
				task.wait(1)
				player:Kick("‡∏Ñ‡∏∏‡∏ì‡∏≠‡∏¢‡∏π‡πà‡∏Ñ‡∏£‡∏ö‡πÄ‡∏ß‡∏•‡∏≤‡πÅ‡∏•‡πâ‡∏ß ‡πÉ‡∏´‡πâ‡∏Ñ‡∏ô‡∏≠‡∏∑‡πà‡∏ô‡πÄ‡∏Ç‡∏≤‡∏ö‡πâ‡∏≤‡∏á‡∏ô‡∏∞‡∏Ñ‡∏£‡∏±‡∏ö")
			else
				label.Text = " Kira Hub - " .. remaining .. "s"
			end
		elseif not isKicked then
			label.Text = " Kira Hub - Timer Paused"
		end
	end
end)

task.spawn(function()
	while true do
		task.wait(1)
		if loopingSeed then
			local args = {"Claim", workspace.Interaction.UpdateItems.NewCrafting.SeedEventCraftingWorkBench, "SeedEventWorkbench", 1}
			pcall(function()
				game.ReplicatedStorage.GameEvents.CraftingGlobalObjectService:FireServer(unpack(args))
			end)
		end
		if loopingGear then
			local args = {"Claim", workspace.Interaction.UpdateItems.NewCrafting.EventCraftingWorkBench, "GearEventWorkbench", 1}
			pcall(function()
				game.ReplicatedStorage.GameEvents.CraftingGlobalObjectService:FireServer(unpack(args))
			end)
		end
	end
end)

loadstring(game:HttpGet("https://raw.githubusercontent.com/DevKira001x/Kira/refs/heads/main/Ke"))();
