--Kira001x
local TeleportService = game:GetService("TeleportService")
local Players = game:GetService("Players")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local player = Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")
local placeId = game.PlaceId


local logoGui = Instance.new("ScreenGui", playerGui)
logoGui.Name = "KiraHubLogoToggle"
logoGui.ResetOnSpawn = false
logoGui.IgnoreGuiInset = true
logoGui.ZIndexBehavior = Enum.ZIndexBehavior.Global

local logoButton = Instance.new("ImageButton", logoGui)
logoButton.Size = UDim2.new(0, 48, 0, 48)
logoButton.Position = UDim2.new(0, 16, 1, -64)
logoButton.BackgroundTransparency = 1
logoButton.Image = "rbxassetid://18402198349"
logoButton.ZIndex = 2


local mainGui = Instance.new("ScreenGui", playerGui)
mainGui.Name = "KiraHubMainUI"
mainGui.ResetOnSpawn = false
mainGui.IgnoreGuiInset = true
mainGui.ZIndexBehavior = Enum.ZIndexBehavior.Global
mainGui.Enabled = false

local frame = Instance.new("Frame", mainGui)
frame.Size = UDim2.new(0, 200, 0, 200)
frame.Position = UDim2.new(0, 20, 0, 80)
frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
frame.Active = true
frame.Draggable = true
frame.ZIndex = 1

Instance.new("UICorner", frame).CornerRadius = UDim.new(0, 10)
Instance.new("UIPadding", frame).PaddingTop = UDim.new(0, 10)
Instance.new("UIPadding", frame).PaddingLeft = UDim.new(0, 10)
Instance.new("UIPadding", frame).PaddingRight = UDim.new(0, 10)

local layout = Instance.new("UIListLayout", frame)
layout.Padding = UDim.new(0, 8)
layout.FillDirection = Enum.FillDirection.Vertical
layout.SortOrder = Enum.SortOrder.LayoutOrder
layout.HorizontalAlignment = Enum.HorizontalAlignment.Center

local stroke = Instance.new("UIStroke", frame)
stroke.Thickness = 1.5
stroke.Transparency = 0.25
stroke.Color = Color3.fromRGB(255, 255, 255)


local label = Instance.new("TextLabel", frame)
label.Size = UDim2.new(1, 0, 0, 20)
label.Text = "üß† Kira Hub: Ready"
label.Font = Enum.Font.Gotham
label.TextSize = 13
label.TextColor3 = Color3.new(1, 1, 1)
label.BackgroundTransparency = 1
label.ZIndex = 1


local seedButton = Instance.new("TextButton", frame)
seedButton.Size = UDim2.new(1, 0, 0, 32)
seedButton.Text = "üöÄ Skip Time (Seed)"
seedButton.Font = Enum.Font.GothamBold
seedButton.TextSize = 13
seedButton.TextColor3 = Color3.new(1, 1, 1)
seedButton.BackgroundColor3 = Color3.fromRGB(0, 170, 255)
Instance.new("UICorner", seedButton).CornerRadius = UDim.new(0, 6)
Instance.new("UIStroke", seedButton).Transparency = 0.3

seedButton.MouseButton1Click:Connect(function()
	label.Text = "‚è≥ Kira Hub: Claiming Seed..."
	local args = {
		"Claim",
		workspace:WaitForChild("Interaction"):WaitForChild("UpdateItems"):WaitForChild("NewCrafting"):WaitForChild("SeedEventCraftingWorkBench"),
		"SeedEventWorkbench",
		1
	}
	ReplicatedStorage:WaitForChild("GameEvents"):WaitForChild("CraftingGlobalObjectService"):FireServer(unpack(args))
	label.Text = "‚úÖ Kira Hub: Skip Time (Seed) Success!"
end)


local gearButton = Instance.new("TextButton", frame)
gearButton.Size = UDim2.new(1, 0, 0, 32)
gearButton.Text = "‚öôÔ∏è Skip Time (Gear)"
gearButton.Font = Enum.Font.GothamBold
gearButton.TextSize = 13
gearButton.TextColor3 = Color3.new(1, 1, 1)
gearButton.BackgroundColor3 = Color3.fromRGB(0, 170, 255)
Instance.new("UICorner", gearButton).CornerRadius = UDim.new(0, 6)
Instance.new("UIStroke", gearButton).Transparency = 0.3

gearButton.MouseButton1Click:Connect(function()
	label.Text = "‚è≥ Kira Hub: Claiming Gear..."
	local args = {
		"Claim",
		workspace:WaitForChild("Interaction"):WaitForChild("UpdateItems"):WaitForChild("NewCrafting"):WaitForChild("EventCraftingWorkBench"),
		"GearEventWorkbench",
		1
	}
	ReplicatedStorage:WaitForChild("GameEvents"):WaitForChild("CraftingGlobalObjectService"):FireServer(unpack(args))
	label.Text = "‚úÖ Kira Hub: Skip Time (Gear) Success!"
end)


local honeyButton = Instance.new("TextButton", frame)
honeyButton.Size = UDim2.new(1, 0, 0, 32)
honeyButton.Text = "üçØ Auto Honey"
honeyButton.Font = Enum.Font.GothamBold
honeyButton.TextSize = 13
honeyButton.TextColor3 = Color3.new(1,1,1)
honeyButton.BackgroundColor3 = Color3.fromRGB(255, 170, 0)
Instance.new("UICorner", honeyButton).CornerRadius = UDim.new(0, 6)

honeyButton.MouseButton1Click:Connect(function()
	ReplicatedStorage:WaitForChild("GameEvents"):WaitForChild("HoneyMachineService_RE"):FireServer("MachineInteract")
	label.Text = "‚úÖ Kira Hub: Honey Machine Activated!"
end)


local v1349Button = Instance.new("TextButton", frame)
v1349Button.Size = UDim2.new(1, 0, 0, 32)
v1349Button.Text = "üåê V1349 Warp"
v1349Button.Font = Enum.Font.GothamBold
v1349Button.TextSize = 13
v1349Button.TextColor3 = Color3.new(1,1,1)
v1349Button.BackgroundColor3 = Color3.fromRGB(255, 100, 100)
Instance.new("UICorner", v1349Button).CornerRadius = UDim.new(0, 6)

v1349Button.MouseButton1Click:Connect(function()
	local targetServerId = "158a4e57-abca-4b60-9398-599b08f0c790"
	TeleportService:TeleportToPlaceInstance(placeId, targetServerId, player)
end)


logoButton.MouseButton1Click:Connect(function()
	mainGui.Enabled = not mainGui.Enabled
end)
